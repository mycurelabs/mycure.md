{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/modules",
        "destination": "/index.html"
      },
      {
        "source": "/pricing",
        "destination": "/index.html"
      },
      {
        "source": "/our-story",
        "destination": "/index.html"
      },
      {
        "source": "/directory",
        "destination": "/index.html"
      },
      {
        "source": "/terms",
        "destination": "/index.html"
      },
      {
        "source": "/privacy-policy",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "function": "doctorWebsite"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "404.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=300"
          }
        ]
      }
    ]
  }
}
